{
    "name": "andrewdyer/predis-request-limiter",
    "description": "Request rate limiting using Predis.",
    "type": "library",
    "keywords": ["php", "redis", "predis", "rate-limit", "framework-agnostic"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Dyer",
            "email": "andrewdyer@outlook.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "predis/predis": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Anddye\\PredisRequestLimiter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anddye\\PredisRequestLimiter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs": [
            "vendor/bin/php-cs-fixer fix"
        ],
        "check": [
            "composer cs"
        ]
    }
}
